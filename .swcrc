      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "minify": {
      "mangle": true
    }
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false
}
